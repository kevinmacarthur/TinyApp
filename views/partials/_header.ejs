<!DOCTYPE html>

<Header>
  <h1> Kevs Lovely TinyApp </h1>
  <% if (username) { %>

  <form action="/urls/logout" method="POST">
    <p> Logged in as <%= username %> </p>
    <input type="submit" value="logout">
  </form>
  <% } else { %>
  <form action="/urls/login" method="POST">
    <input name="username" value="" placeholder="Enter Username" style="width: 200px">
    <input type="submit" value="Sign In">
  </form>
  <% } %>
</header>


